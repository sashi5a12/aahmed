<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.netpace.vzdn.model">
    <class name="Report" table="vzdn_reports">
        <id name="reportId" type="java.lang.Integer">
            <column name="report_id" />
            <generator class="native" />
        </id>
        <many-to-one name="privilege" class="VzdnSysPrivileges" fetch="join" not-null="true" lazy="false">
            <column name="privlege_id" />
        </many-to-one>
        
        <property name="displayName" type="java.lang.String">
            <column name="display_name" length="100" />
        </property>
        <property name="reportAction" type="java.lang.String">
            <column name="report_action" length="100" />
        </property>
        
        <property name="pdfExport" type="java.lang.Character">
            <column name="pdf_export" length="1" />
        </property>
        
        <property name="cvsExport" type="java.lang.Character">
            <column name="cvs_export" length="1" />
        </property>
        <property name="rtfExport" type="java.lang.Character">
            <column name="rtf_export" length="1" />
        </property>
        <property name="htmlExport" type="java.lang.Character">
            <column name="html_export" length="1" />
        </property>
        
        <property name="createdBy" type="java.lang.String">
            <column name="created_by" length="100" />
        </property>
        <property name="createdDate" type="java.sql.Timestamp">
            <column name="created_date" length="19" />
        </property>
        <property name="lastUpdatedBy" type="java.lang.String">
            <column name="last_updated_by" length="100" />
        </property>
        <property name="lastUpdatedDate" type="java.sql.Timestamp">
            <column name="last_updated_date" length="19" />
        </property>
        
    </class>
</hibernate-mapping>
