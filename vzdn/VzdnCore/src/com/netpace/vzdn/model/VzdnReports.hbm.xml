<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.netpace.vzdn.model">
    <class name="VzdnReports" table="vzdn_reports">
        <id name="reportId" type="java.lang.Integer">
            <column name="report_id" />
            <generator class="native" />
        </id>
        <property name="name" type="java.lang.String">
            <column name="name" length="100" />
        </property>
        <property name="description" type="java.lang.String">
            <column name="description" length="500" />
        </property>
        <property name="reportFile" type="java.lang.String">
            <column name="report_file" length="100" />
        </property>
        <property name="pdfExport" type="java.lang.String">
            <column name="pdf_export" length="1" />
        </property>
        <property name="cvsExport" type="java.lang.String">
            <column name="cvs_export" length="1" />
        </property>
        <property name="rtfExport" type="java.lang.String">
            <column name="rtf_export" length="1" />
        </property>
        <property name="htmlExport" type="java.lang.String">
            <column name="html_export" length="1" />
        </property>
        <property name="createdBy" type="java.lang.String">
            <column name="created_by" length="100" />
        </property>
        <property name="createdDate" type="java.sql.Timestamp">
            <column name="created_date" length="19" />
        </property>
        <property name="lastUpdatedBy" type="java.lang.String">
            <column name="last_updated_by" length="100" />
        </property>
        <property name="lastUpdatedDate" type="java.sql.Timestamp">
            <column name="last_updated_date" length="19" />
        </property>
        <set name="roles" table="vzdn_report_roles" inverse="true" lazy="true" cascade="save-update">
            <key>
                <column name="report_id" not-null="true" />
            </key>
            <many-to-many class="VzdnSysRoles">
                <column name="role_id" not-null="true" />
            </many-to-many>
        </set>
    </class>
</hibernate-mapping>
