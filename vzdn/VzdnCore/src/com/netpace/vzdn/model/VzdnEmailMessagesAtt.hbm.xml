<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.netpace.vzdn.model">
    <class name="VzdnEmailMessagesAtt" table="vzdn_email_messages_att" >
    	<id name="emailMessageId" type="java.lang.Integer" column="email_message_id">
	        <generator class="assigned" />
    	</id>    
        <property name="att" type="java.sql.Blob">
            <column name="att" />
        </property>
        <property name="attFileName" type="java.lang.String">
            <column name="att_file_name" length="150" />
        </property>
        <property name="attContentType" type="java.lang.String">
            <column name="att_content_type" length="100" />
        </property>
        <property name="createdBy" type="java.lang.String">
            <column name="created_by" length="50" />
        </property>
        <property name="createdDate" type="java.sql.Timestamp">
            <column name="created_date" length="19" />
        </property>
    </class>
</hibernate-mapping>
