<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.netpace.vzdn.model">
    <class name="VzdnTypeDefs" table="vzdn_type_defs">
        <id name="typeDefId" type="java.lang.Integer">
            <column name="type_def_id" />
            <generator class="native" />
        </id>
        <property name="typeName" type="java.lang.String">
            <column name="type_name" length="100" not-null="true" />
        </property>
        <property name="typeDescription" type="java.lang.String">
            <column name="type_description" length="500" />
        </property>
        <property name="createdBy" type="java.lang.String">
            <column name="created_by" length="50" />
        </property>
        <property name="createdDate" type="java.sql.Timestamp">
            <column name="created_date" length="19" />
        </property>
        <property name="lastUpdatedBy" type="java.lang.String">
            <column name="last_updated_by" length="50" />
        </property>
        <property name="lastUpdatedDate" type="java.sql.Timestamp">
            <column name="last_updated_date" length="19" />
        </property>
        <set name="vzdnTypes" inverse="true" lazy="true">
            <key>
                <column name="type_def_id" not-null="true" />
            </key>
            <one-to-many class="VzdnTypes" />
        </set>
    </class>
</hibernate-mapping>
