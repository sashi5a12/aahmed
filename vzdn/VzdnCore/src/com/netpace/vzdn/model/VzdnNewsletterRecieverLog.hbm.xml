<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.netpace.vzdn.model">
				 
    <class name="VzdnNewsletterRecieverLog" table="vzdn_newsletter_reciever_log">
    
        <id name="newsletterRecieverId" type="java.lang.Integer">
            <column name="newsletter_reciever_log_id" />
            <generator class="native" />
        </id>             
        <property name="emailAddress" type="java.lang.String">
            <column name="email_address" length="100"/>
        </property>
        
         <many-to-one name="vzdnNewsletterEmailLog" class="VzdnNewsletterEmailLog" fetch="join">
            <column name="newsletter_email_log_id" />
            
        </many-to-one>
        
    </class>
</hibernate-mapping>
