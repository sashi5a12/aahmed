<class 
    name="com.netpace.aims.model.system.AimsSalesContact" 
    table="AIMS_SALES_CONTACTS"
>
    <meta attribute="extends">com.netpace.aims.model.BaseValueObject</meta>
    <id
        name="salesContactId"
        type="java.lang.Long"
        column="SALES_CONTACT_ID"
    >
        <generator class="sequence">
             <param name="sequence">SEQ_PK_SALES_CONTACTS</param>
        </generator>
    </id>
    <property
        name="regionId"
        type="java.lang.Long"
        column="REGION_ID"
    />
	
	    <property
        name="createdBy"
        type="java.lang.String"
        column="CREATED_BY"
        length="50"
    />

     <property
        name="createdDate"
        type="java.sql.Timestamp"
        column="CREATED_DATE"
        length="7"
    />
    <property
        name="lastUpdatedBy"
        type="java.lang.String"
        column="LAST_UPDATED_BY"
        length="50"
    />
    <property
        name="lastUpdatedDate"
        type="java.sql.Timestamp"
        column="LAST_UPDATED_DATE"
        length="7"
    />

    <!-- associations -->
    <!-- bi-directional many-to-one association to AimsContact -->
    <many-to-one
        name="aimsContact"
        class="com.netpace.aims.model.core.AimsContact"
        not-null="true" cascade="save-update">
        <column name="CONTACT_ID" />
    </many-to-one>   


</class>
</hibernate-mapping>
