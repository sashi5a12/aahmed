<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>
	<class name="com.netpace.aims.model.system.AimsDeviceFirmware" table="AIMS_DEVICE_FIRMWARE">
		<meta attribute="extends">com.netpace.aims.model.BaseValueObject</meta>
		<id 
			name="firmwareId" 
			type="java.lang.Long" 
			column="FIRMWARE_ID">
			
			<generator class="sequence">
				<param name="sequence">SEQ_PK_FIRMWARE_ID</param>
			</generator>
		</id>		
		
		<property 
				name="deviceId" 
				type="java.lang.Long" 
				column="DEVICE_ID" 
				not-null="true" />

		<property 
				name="firmwareName" 
				type="java.lang.String" 
				column="FIRMWARE_NAME" 
				length="250" />

		<property 
				name="mrNumber" 
				type="java.lang.Integer" 
				column="MR_NUMBER" />

		<property 
				name="status" 
				type="java.lang.String" 
				column="STATUS" 
				length="10" />

		<property 
				name="description" 
				type="java.lang.String" 
				column="DESCRIPTION" 
				length="255" />
		<property 
		        name="createdBy"
		        type="java.lang.String"
		        column="CREATED_BY"
		        length="50"
		        update="false" />
		<property
		        name="createdDate"
		        type="java.sql.Timestamp"
		        column="CREATED_DATE"
		        length="7" 
		        update="false" />
		<property
		        name="lastUpdatedBy"
		        type="java.lang.String"
		        column="LAST_UPDATED_BY"
		        length="50" />
		<property
		        name="lastUpdatedDate"
		        type="java.sql.Timestamp"
		        column="LAST_UPDATED_DATE"
		        length="7" />			
	</class>
</hibernate-mapping>
