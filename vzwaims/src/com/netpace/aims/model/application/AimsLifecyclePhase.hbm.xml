<class 
    name="com.netpace.aims.model.application.AimsLifecyclePhase" 
    table="AIMS_LIFECYCLE_PHASES"
>
	<meta attribute="extends">com.netpace.aims.model.BaseValueObject</meta>    
    <id
        name="phaseId"
        type="java.lang.Long"
        column="PHASE_ID"
    >
        <generator class="sequence">
			<param name="sequence">SEQ_PK_LIFECYCLE_PHASES</param>
		</generator>
    </id>
    <property
        name="phaseName"
        type="java.lang.String"
        column="PHASE_NAME"
        length="100"
    />
    <property
        name="phaseDesc"
        type="java.lang.String"
        column="PHASE_DESC"
        length="200"
    />
    <property
        name="createdBy"
        type="java.lang.String"
        column="CREATED_BY"
        length="50"
    />
    <property
        name="createdDate"
        type="java.sql.Timestamp"
        column="CREATED_DATE"
        length="7"
    />
    <property
        name="lastUpdatedBy"
        type="java.lang.String"
        column="LAST_UPDATED_BY"
        length="50"
    />
    <property
        name="lastUpdatedDate"
        type="java.sql.Timestamp"
        column="LAST_UPDATED_DATE"
        length="7"
    />

    <!-- associations -->
    <!-- bi-directional one-to-many association to AimsApp
    <set
        name="aimsApps"
        lazy="true"
        inverse="true"
    >
        <key>
            <column name="PHASE_ID" />
        </key>
        <one-to-many 
            class="com.netpace.aims.model.AimsApp"
        />
    </set>
