<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>
    <class name="com.netpace.aims.model.application.AimsBrewMessagingDetails" table="AIMS_BREW_MESSAGING_DETAILS">
        <id name="messageDetailId" type="java.lang.Long">
            <column name="MESSAGE_DETAIL_ID"/>
	        <generator class="sequence">
				<param name="sequence">SEQ_BREW_MESSAGING_DETAILS</param>
			</generator>
        </id>
        <property name="brewAppId" type="java.lang.Long">
            <column name="BREW_APP_ID"/>
        </property>
        <property name="aggregator" type="java.lang.Long">
            <column name="AGGREGATOR"/>
        </property>                
        <property name="applicationName" type="java.lang.String">
            <column name="APPLICATION_NAME" length="500" />
        </property>
        <property name="msgCategory" type="java.lang.String">
            <column name="MSG_CATEGORY" length="100" />
        </property>
        <property name="shortCode" type="java.lang.String">
            <column name="SHORT_CODE" length="50" />
        </property>
        <property name="keyword" type="java.lang.String">
            <column name="KEYWORD" length="100" />
        </property>
        <property name="createdBy" type="java.lang.String">
            <column name="CREATED_BY" length="200" />
        </property>
        <property name="createdDate" type="java.sql.Timestamp">
            <column name="CREATED_DATE" length="7" />
        </property>
        <property name="modifiedBy" type="java.lang.String">
            <column name="MODIFIED_BY" length="200" />
        </property>
        <property name="modifiedDate" type="java.sql.Timestamp">
            <column name="MODIFIED_DATE" length="7" />
        </property>
    </class>
</hibernate-mapping>
