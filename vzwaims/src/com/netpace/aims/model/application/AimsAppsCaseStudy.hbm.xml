<class 
    name="com.netpace.aims.model.application.AimsAppsCaseStudy" 
    table="AIMS_APPS_CASE_STUDIES"
>
	<meta attribute="extends">com.netpace.aims.model.BaseValueObject</meta>
    <id
        name="caseStudyId"
        type="java.lang.Long"
        column="CASE_STUDY_ID"
    >
      <generator class="sequence">
			<param name="sequence">SEQ_PK_APPS_CASE_STUDIES</param>
		</generator>
    </id>

    <property
        name="customerName"
        type="java.lang.String"
        column="CUSTOMER_NAME"
        length="100"
    />
    <property
        name="problemStatement"
        type="java.lang.String"
        column="PROBLEM_STATEMENT"
        length="100"
    />
    <property
        name="businessBenifit"
        type="java.lang.String"
        column="BUSINESS_BENIFIT"
        length="100"
    />
    <property
        name="productionLaunchDate"
        type="java.sql.Timestamp"
        column="PRODUCTION_LAUNCH_DATE"
        length="7"
    />
    <property
        name="numUsers"
        type="java.lang.Long"
        column="NUM_USERS"
        length="22"
    />
    <property
        name="numWirelessUsers"
        type="java.lang.Long"
        column="NUM_WIRELESS_USERS"
        length="22"
    />
    <property
        name="platformId"
        type="java.lang.Long"
        column="PLATFORM_ID"
        length="22"
    />

    <!-- associations -->
	 <many-to-one name="aimsEnterpriseApp" 
	 class="com.netpace.aims.model.application.AimsEnterpriseApp" 
	 column="ENTERPRISE_APPS_ID" not-null="true"/>
	 
</class>
