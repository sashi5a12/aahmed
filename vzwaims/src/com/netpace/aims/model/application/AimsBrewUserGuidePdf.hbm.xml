<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >

<hibernate-mapping>
	<class name="com.netpace.aims.model.application.AimsBrewUserGuidePdf" table="AIMS_BREW_APPS">
		<meta attribute="extends"> com.netpace.aims.model.BaseValueObject </meta>
		<id name="brewAppsId" type="java.lang.Long" column="BREW_APPS_ID">
			<generator class="assigned" />
		</id>

		<property name="userGuide" type="java.sql.Blob" column="USER_GUIDE" />
		<property name="userGuideFileName" type="java.lang.String" column="USER_GUIDE_FILE_NAME" length="150" />
		<property name="userGuideContentType" type="java.lang.String" column="USER_GUIDE_CONTENT_TYPE" length="100" />
	</class>
</hibernate-mapping>
