<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >
    
<hibernate-mapping>

<class 
    name="com.netpace.aims.model.application.AimsJavaFiles" 
    table="AIMS_JAVA_APPS"
>
		<meta attribute="extends">com.netpace.aims.model.BaseValueObject</meta>    
    <id
        name="javaAppsId"
        type="java.lang.Long"
        column="JAVA_APPS_ID"
    >
        <generator class="assigned"/>			
    </id>    
    
    <property
        name="clrPubLogo"
        type="java.sql.Blob"
        column="HR_PUBLISHER_LOGO"
        length="4000"
    />
    <property
        name="clrPubLogoFileName"
        type="java.lang.String"
        column="HR_PUBLISHER_LOGO_FILE_NAME"
        length="150"
    />
    <property
        name="clrPubLogoContentType"
        type="java.lang.String"
        column="HR_PUBLISHER_LOGO_CONTENT_TYPE"
        length="100"
    />
    
    <property
        name="appTitleName"
        type="java.sql.Blob"
        column="CHNL_TITLE_ICON"
        length="4000"
    />
    <property
        name="appTitleNameFileName"
        type="java.lang.String"
        column="CHNL_TITLE_ICON_FILE_NAME"
        length="150"
    />
    <property
        name="appTitleNameContentType"
        type="java.lang.String"
        column="CHNL_TITLE_ICON_CONTENT_TYPE"
        length="100"
    />    

    <property 
    	name="companyLogo" 
    	type="java.sql.Blob"
    	column="COMPANY_LOGO"
    	length="4000"
    />    
    <property 
    	name="companyLogoFileName" 
    	type="java.lang.String"
    	column="COMPANY_LOGO_FILE_NAME" 
    	length="150"
    />
    <property 
    	name="companyLogoContentType" 
    	type="java.lang.String"
    	column="COMPANY_LOGO_CONTENT_TYPE" 
    	length="100" 
    />    
    
    <property 
    	name="titleImage" 
    	type="java.sql.Blob"
    	column="TITLE_IMAGE"
    	length="4000"
    />    
    <property 
    	name="titleImageFileName" 
    	type="java.lang.String"
    	column="TITLE_IMAGE_FILE_NAME" 
    	length="150"
    />
    <property 
    	name="titleImageContentType" 
    	type="java.lang.String"
    	column="TITLE_IMAGE_CONTENT_TYPE" 
    	length="100" 
    />
    
</class>
</hibernate-mapping>
