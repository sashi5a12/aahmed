<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >
    
<hibernate-mapping>

<!-- 
    Created by Middlegen Hibernate plugin

    http://boss.bekk.no/boss/middlegen/
    http://hibernate.sourceforge.net/
-->

<class 
    name="com.netpace.aims.model.application.AimsCatalogsData" 
    table="AIMS_CATALOGS_DATA"
>
    <meta attribute="extends">com.netpace.aims.model.BaseValueObject</meta>  
	<id
        name="catalogDataId"
        type="java.lang.Long"
        column="CATALOG_DATA_ID"
    >
        <generator class="sequence">
             <param name="sequence">SEQ_PK_CATALOGS_DATA</param>
        </generator>
    </id>
    
    <property
        name="catalogId"
        type="java.lang.Long"
        column="CATALOG_ID"
        update="false"
        insert="false"
    />
    
    <property
        name="applicationCategory"
        type="java.lang.String"
        column="APPLICATION_CATEGORY"
        length="100"
    />
    <property
        name="developerName"
        type="java.lang.String"
        column="DEVELOPER_NAME"
        length="200"
    />
    <property
        name="applicationName"
        type="java.lang.String"
        column="APPLICATION_NAME"
        length="200"
    />
    <property
        name="partNumber"
        type="java.lang.String"
        column="PART_NUMBER"
        length="50"
    />
    <property
        name="handset"
        type="java.lang.String"
        column="HANDSET"
        length="30"
    />
    <property
        name="version"
        type="java.lang.String"
        column="VERSION"
        length="10"
    />
    <property
        name="platformId"
        type="java.lang.String"
        column="PLATFORM_ID"
        length="200"
    />
    <property
        name="notes"
        type="java.lang.String"
        column="NOTES"
        length="500"
    />

    <property
        name="toWcDate"
        type="java.sql.Timestamp"
        column="TO_WC_DATE"
        length="7"
    />
    <property
        name="bdsAcceptanceDate"
        type="java.sql.Timestamp"
        column="BDS_ACCEPTANCE_DATE"
        length="7"
    />
    <property
        name="brewAppsId"
        type="java.lang.Long"
        column="BREW_APPS_ID"
        length="22"
    />
    <property
        name="deviceId"
        type="java.lang.Long"
        column="DEVICE_ID"
        length="22"
    />

    <!-- associations -->
    <!-- bi-directional many-to-one association to AimsCatalog -->
    <many-to-one
        name="aimsCatalog"
        class="com.netpace.aims.model.application.AimsCatalog"
        not-null="true"
    >
        <column name="CATALOG_ID" />
    </many-to-one>   

</class>
</hibernate-mapping>
