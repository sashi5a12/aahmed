<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >
    
<hibernate-mapping>

<!-- 
    Created by Middlegen Hibernate plugin

    http://boss.bekk.no/boss/middlegen/
    http://hibernate.sourceforge.net/
-->

<class 
    name="com.netpace.aims.model.application.AimsAppPhaseFiles" 
    table="AIMS_APP_PHASES"
>
	<meta attribute="extends">com.netpace.aims.model.BaseValueObject</meta>    

		<composite-id>
      <key-property name="appsId" column="APPS_ID"/>
      <key-many-to-one name="aimsVzwTestingPhase" class="com.netpace.aims.model.application.AimsVzwTestingPhase" column="TESTING_PHASE_ID"/>
    </composite-id>
    
    <property name="appsId" type="java.lang.Long" column="APPS_ID" insert="false" update="false" /> 
    
    <property
        name="results"
        type="java.sql.Blob"
        column="RESULTS"
        length="4000"
    />
    <property
        name="resultsFileName"
        type="java.lang.String"
        column="RESULTS_FILE_NAME"
        length="150"
    />
    <property
        name="resultsContentType"
        type="java.lang.String"
        column="RESULTS_CONTENT_TYPE"
        length="100"
    />




</class>
</hibernate-mapping>


