<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>
	<class name="com.netpace.aims.model.application.AimsVZAppMPortalXMLLog" table="AIMS_VZAPP_MPORTAL_XML_LOG">
		<meta attribute="extends">com.netpace.aims.model.BaseValueObject</meta>
		<id 
			name="logId" 
			type="java.lang.Long"
			column="LOG_ID"
			>
			<generator class="sequence">
				<param name="sequence">
					SEQ_PK_MPORTAL_XML_LOG
				</param>
			</generator>

		</id>
		<property 
			name="appId" 
			type="java.lang.Long" 
			column="APP_ID" />		
		<property 
			name="submitStatus" 
			type="java.lang.String" 
			column="SUBMIT_STATUS"
			length="1"/>
		<property 
			name="submitResponse" 
			type="java.lang.String"
			column="SUBMIT_RESPONSE"
			length="2000"/>
		<property 
			name="submitXml" 
			type="java.sql.Clob"
			column="SUBMIT_XML"/>
		<property 
			name="submitDate" 
			type="java.util.Date"
			column="SUBMIT_DATE" 
			length="7" />
		<property 
			name="xmlType" 
			type="java.lang.String"
			column="XML_TYPE"
			length="10"/>
		<property 
			name="binaryIds" 
			type="java.lang.String"
			column="BINARY_IDS"
			length="1000"/>
		<property 
			name="binaryFirmwareIds" 
			type="java.lang.String"
			column="BINARY_FIRMWARE_IDS"
			length="1000"/>					
	</class>
</hibernate-mapping>
