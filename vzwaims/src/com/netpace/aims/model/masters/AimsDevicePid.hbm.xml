<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>
    <class name="com.netpace.aims.model.masters.AimsDevicePid" table="AIMS_DEVICE_PID">
	    <meta attribute="extends">com.netpace.aims.model.BaseValueObject</meta>
        <id name="devicePidId" type="java.lang.Long">
            <column name="DEVICE_PID_ID"/>
				<generator class="sequence">
					<param name="sequence">SEQ_PK_DEVICE_PID</param>
				</generator>
        </id>
        <property name="deviceId" type="java.lang.Long">
            <column name="DEVICE_ID" not-null="true" />
        </property>
        <property name="pid" type="java.lang.String">
            <column name="PID" length="50" not-null="true" unique="true" />
        </property>
        <property name="sid1" type="java.lang.String">
            <column name="SID1" length="50" not-null="true" unique="true" />
        </property>        
        <property name="sid2" type="java.lang.String">
            <column name="SID2" length="50" not-null="true" unique="true" />
        </property>        
        <property name="sid3" type="java.lang.String">
            <column name="SID3" length="50" not-null="true" unique="true" />
        </property>        
    </class>
</hibernate-mapping>
