<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >
    
<hibernate-mapping>

<!-- 
    Created by Middlegen Hibernate plugin

    http://boss.bekk.no/boss/middlegen/
    http://hibernate.sourceforge.net/
-->

<class 
    name="com.netpace.aims.model.content.AimsFaqCategory" 
    table="AIMS_FAQ_CATEGORIES"
    dynamic-update="true"
>
    <meta attribute="extends">com.netpace.aims.model.BaseValueObject</meta>
    <id
        name="faqCategoryId"
        type="java.lang.Long"
        column="FAQ_CATEGORY_ID"
    >
        <generator class="sequence">
             <param name="sequence">SEQ_PK_FAQ_CATEGORIES</param>
        </generator>
    </id>
    <property
        name="faqCategoryName"
        type="java.lang.String"
        column="FAQ_CATEGORY_NAME"
        length="200"
    >
        <meta attribute="use-in-tostring">true</meta>
    </property>
    <property
        name="faqCategoryDesc"
        type="java.lang.String"
        column="FAQ_CATEGORY_DESC"
        length="500"
    />
    <property
        name="platformId"
        type="java.math.BigDecimal"
        column="PLATFORM_ID"
        length="22"
    />
    <property
        name="createdBy"
        type="java.lang.String"
        column="CREATED_BY"
        length="50"
    />
    <property
        name="createdDate"
        type="java.sql.Timestamp"
        column="CREATED_DATE"
        length="7"
    />
    <property
        name="lastUpdatedBy"
        type="java.lang.String"
        column="LAST_UPDATED_BY"
        length="50"
    />
    <property
        name="lastUpdatedDate"
        type="java.sql.Timestamp"
        column="LAST_UPDATED_DATE"
        length="7"
    />

    <!-- associations -->
    <!-- bi-directional one-to-many association to AimsFaqTopic -->
    <set
        name="aimsFaqTopics"
        lazy="true"
        inverse="true"
    >
        <key>
            <column name="FAQ_CATEGORY_ID" />
        </key>
        <one-to-many 
            class="com.netpace.aims.model.content.AimsFaqTopic"
        />
    </set>

</class>
</hibernate-mapping>
