<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >
    
<hibernate-mapping>
    <class name="com.netpace.aims.model.wapoptout.AimsWapOptoutFileLog" table="aims_wap_optout_whitelistlog">
    	<meta attribute="extends">com.netpace.aims.model.BaseValueObject</meta>
        <id name="logId" type="java.lang.Long">
            <column name="log_id"/>
	        <generator class="sequence">
				<param name="sequence">seq_pk_whitelistlog_id</param>
			</generator>
        </id>
        <property name="createdDate" type="java.sql.Timestamp">
            <column name="log_timestamp" />
        </property>

        <property name="whitelistFile" type="java.sql.Clob">
            <column name="whitelist_file" /> 
        </property>
    </class>
</hibernate-mapping>