<SCRIPT LANGUAGE="JavaScript">

function NewWindow(mypage, myname, w, h, scroll)
{
   var winl = (screen.width - w) / 2;
   var wint = (screen.height - h) / 2;

   var agt=navigator.userAgent.toLowerCase();
   var is_aol   = (agt.indexOf("aol 6") != -1);
   if (is_aol) {
       winprops = 'height='+h+',width='+w+',scrollbars='+scroll+''
       win = window.open(mypage, myname, winprops)
   }else {
       winprops = 'height='+h+',width='+w+',top='+wint+',left='+winl+',scrollbars='+scroll+''
       win = window.open(mypage, myname, winprops)
   }
    if (parseInt(navigator.appVersion) >= 4) { win.focus(); }

}

function NewWinWithResize(mypage, myname, w, h, scroll, resiz)
{
   var winl = (screen.width - w) / 2;
   var wint = (screen.height - h) / 2;

   var agt=navigator.userAgent.toLowerCase();
   var is_aol   = (agt.indexOf("aol 6") != -1);
   if (is_aol) {
       winprops = 'height='+h+',width='+w+',scrollbars='+scroll+',rezizable='+resiz+''
       win = window.open(mypage, myname, winprops)
   }else {
       winprops = 'height='+h+',width='+w+',top='+wint+',left='+winl+',scrollbars='+scroll+',resizable='+resiz+''
       win = window.open(mypage, myname, winprops)
   }
    if (parseInt(navigator.appVersion) >= 4) { win.focus(); }

}

function popTour() {
   tourWin = window.open('tour_1.htm','newWin','width=500,height=540');
   if (parseInt(navigator.appVersion) >= 4) { tourWin.window.focus(); }
}
</script>
--%>
