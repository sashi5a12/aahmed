<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="validationFramework" namespace="/validationFramework"	extends="struts-default">
		<action name="Registration">
			<result>/jsps/validationFramework/Registration.jsp</result>
		</action>
		<action name="Submit" class="validationFramework.Registration">
			<result>/jsps/validationFramework/Success.jsp</result>
			<result name="input">/jsps/validationFramework/Registration.jsp</result>
		</action>
	</package>
</struts>