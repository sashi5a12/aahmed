<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="fileupload" namespace="/fileupload"	extends="struts-default">
		<action name="AddFile">
			<result>/jsps/fileupload/AddFile.jsp</result>
		</action>
		<action name="FileUpload" class="fileupload.FileUpload">
			<param name="fileSystemPath">C://</param>
			<result>/jsps/fileupload/FileAdded.jsp</result>
			<result name="input">/jsps/fileupload/AddFile.jsp</result>
		</action>
	</package>
</struts>