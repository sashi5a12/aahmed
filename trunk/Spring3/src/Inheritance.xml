<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
	
	<bean name="parent" class="net.roseindia.Bean">
		<property name="name" value="Adnan Ahmed"></property>
	</bean>
	
	<bean name="child" class="net.roseindia.Bean" parent="parent">
		<property name="address" value="Fremont"></property>
	</bean>
	
	<bean name="subchild" class="net.roseindia.Bean" parent="parent">
	</bean>
</beans>       
