<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>
	<display-name>VzAppZone Firmware Binary</display-name>
	<distributable />
	<servlet>
		<servlet-name>binaryServlet</servlet-name>
		<servlet-class>com.netpace.aims.servlet.BinaryServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>binaryServlet</servlet-name>
		<url-pattern>/getBinary</url-pattern>
	</servlet-mapping>	
	<welcome-file-list>
		<welcome-file>/index.jsp</welcome-file>
	</welcome-file-list>
	<error-page>
		<error-code>404</error-code>
		<location>/index.jsp</location>
	</error-page>
</web-app>
