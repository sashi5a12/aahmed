<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>
    <class name="com.netpace.aims.model.system.AimsDisclaimers" table="AIMS_DISCLAIMERS">
        <id name="disclaimerId" type="java.lang.Long">
        	<meta attribute="use-in-tostring">true</meta>        	
            <column name="DISCLAIMER_ID" />
            <generator class="assigned"></generator>
        </id>
        <property name="disclaimerName" type="java.lang.String">
        	<meta attribute="use-in-tostring">true</meta>        
            <column name="DISCLAIMER_NAME" length="100" />
        </property>
        <property name="disclaimerText" type="java.sql.Clob">
            <column name="DISCLAIMER_TEXT"/>
        </property>
        <property name="disclaimerStr" type="java.lang.String" formula="DISCLAIMER_ID"/>
        <property name="createdDate" type="java.util.Date" update="false">
            <column name="CREATED_DATE" length="7" />
        </property>
        <property name="createdBy" type="java.lang.String" update="false">
            <column name="CREATED_BY" length="200" />
        </property>
        <property name="modifiedDate" type="java.util.Date">
            <column name="MODIFIED_DATE" length="7" />
        </property>
        <property name="modifiedBy" type="java.lang.String">
            <column name="MODIFIED_BY" length="200" />
        </property>
    </class>
</hibernate-mapping>
