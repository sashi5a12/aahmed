<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>
    <class name="com.netpace.aims.model.tools.AimsToolFetchXlsLog" table="AIMS_TOOL_FETCH_XLS_LOG">
        <id name="logId" type="java.lang.Long">
            <column name="LOG_ID"/>
            <generator class="sequence">
            	<param name="sequence">SEQ_PK_TOOL_FETCH_XLS_LOG</param>
            </generator>
        </id>
        <property name="fetchFile" type="java.sql.Blob">
            <column name="FETCH_FILE" />
        </property>
        <property name="description" type="java.lang.String">
            <column name="DESCRIPTION" length="500" />
        </property>
        <property name="createdDate" type="java.util.Date">
            <column name="CREATED_DATE" length="7" />
        </property>
        <property name="createdBy" type="java.lang.String">
            <column name="CREATED_BY" length="200" />
        </property>
    </class>
</hibernate-mapping>
