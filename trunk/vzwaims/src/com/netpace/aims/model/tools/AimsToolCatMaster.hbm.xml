<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >
    
<hibernate-mapping>

<!-- 
    Created by Middlegen Hibernate plugin

    http://boss.bekk.no/boss/middlegen/
    http://hibernate.sourceforge.net/
-->

<class 
    name="com.netpace.aims.model.tools.AimsToolCatMaster" 
    table="AIMS_TOOL_CAT_MASTER"
>
<meta attribute="extends">com.netpace.aims.model.BaseValueObject</meta>
    <id
        name="categoryId"
        type="java.lang.Long"
        column="CATEGORY_ID"
    >
          <generator class="sequence">
             <param name="sequence">SEQ_PK_TOOL_CAT_MASTER</param>
        </generator>    
	</id>
    <property
        name="categoryName"
        type="java.lang.String"
        column="CATEGORY_NAME"
        length="100"
    />
    <property
        name="categoryDesc"
        type="java.lang.String"
        column="CATEGORY_DESC"
        length="500"
    />
    <property
        name="createdBy"
        type="java.lang.String"
        column="CREATED_BY"
        length="50"
    />
    <property
        name="createdDate"
        type="java.sql.Timestamp"
        column="CREATED_DATE"
        length="7"
    />
    <property
        name="lastUpdatedBy"
        type="java.lang.String"
        column="LAST_UPDATED_BY"
        length="50"
    />
    <property
        name="lastUpdatedDate"
        type="java.sql.Timestamp"
        column="LAST_UPDATED_DATE"
        length="7"
    />

    <!-- associations -->
    <!-- bi-directional one-to-many association to AimsToolCategory -->
    <set
        name="aimsToolCategories"
        lazy="true"
        inverse="true"
    >
        <key>
            <column name="CATEGORY_ID" />
        </key>
        <one-to-many 
            class="com.netpace.aims.model.tools.AimsToolCategory"
        />
    </set>

</class>
</hibernate-mapping>
