<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping>
	<!-- 
    Created by Middlegen Hibernate plugin

    http://boss.bekk.no/boss/middlegen/
    http://hibernate.sourceforge.net/
-->
	<class name="com.netpace.aims.model.core.AimsUser" table="AIMS_USERS" dynamic-update="true" >
		<meta attribute="extends">com.netpace.aims.model.BaseValueObject</meta>
		<id name="userId" type="java.lang.Long" column="USER_ID">
			<generator class="sequence">
				<param name="sequence">SEQ_PK_USERS</param>
			</generator>
		</id>
		<property name="username" type="java.lang.String" column="USERNAME" not-null="true" length="20">
			<meta attribute="use-in-tostring">true</meta>
		</property>
		<property name="password" type="java.lang.String" column="PASSWORD" not-null="true" length="50"/>
		<property name="userType" type="java.lang.String" column="USER_TYPE" not-null="true" length="20"/>
		<property name="motherMaidenName" type="java.lang.String" column="MOTHER_MAIDEN_NAME" length="50"/>
		<property name="managerId" type="java.lang.Long" column="MANAGER_ID" length="22"/>
		<property name="ifHq" type="java.lang.String" column="IF_HQ" length="1"/>
		<property name="userAccountStatus" type="java.lang.String" column="USER_ACCOUNT_STATUS" length="10"/>
		<property name="userSessionStatus" type="java.lang.String" column="USER_SESSION_STATUS" length="10"/>
		<property name="ifBlocked" type="java.lang.String" column="IF_BLOCKED" length="1"/>
		<property name="lastLoginDate" type="java.sql.Timestamp" column="LAST_LOGIN_DATE" length="7"/>
		<property name="createdBy" type="java.lang.String" column="CREATED_BY" length="50"/>
		<property name="createdDate" type="java.sql.Timestamp" column="CREATED_DATE" length="7"/>
		<property name="lastUpdatedBy" type="java.lang.String" column="LAST_UPDATED_BY" length="50"/>
		<property name="lastUpdatedDate" type="java.sql.Timestamp" column="LAST_UPDATED_DATE" length="7"/>
		<property name="notificationType" type="java.lang.String" column="NOTIFICATION_TYPE" length="5"/>
		<property name="rowsLength" type="java.lang.Long" column="ROWS_LENGTH" length="22"/>
		<property name="isAccountManager" type="java.lang.String" column="IS_ACCOUNT_MANAGER" length="1"/>
        <property name="permanentlyDeleted" type="java.lang.String" column="PERMANENTLY_DELETED" length="1"/>

        <!--many-to-one association CHANGED to properties-->
    	<property name="aimsAllianc" type="java.lang.Long" column="ALLIANCE_ID" length="22"    />
    	
    	<!-- Added for not directly accessing the child object property -->
    	<property name="aimsContactId" type="java.lang.Long" column="CONTACT_ID" length="22"  insert="false" update="false"  />
		
		<!-- associations -->
		<!-- bi-directional many-to-one association to AimsContact -->
		<many-to-one name="aimsContact" class="com.netpace.aims.model.core.AimsContact" not-null="true" cascade="save-update">
			<column name="CONTACT_ID"/>
		</many-to-one>
		<!-- bi-directional many-to-one association to AimsVzwDepartment -->
		<many-to-one name="aimsVzwDepartment" class="com.netpace.aims.model.core.AimsVzwDepartment" not-null="true">
			<column name="DEPARTMENT_ID"/>
		</many-to-one>
		
    <!-- bi-directional one-to-many association to AimsUserRole -->
    <set name="roles" table="AIMS_USER_ROLES">
        <key>
            <column name="USER_ID" not-null="true" />
        </key>
        <many-to-many class="com.netpace.aims.model.security.AimsSysRole">
         <column name="ROLE_ID" not-null="true"/>
        </many-to-many>
    </set>
		
		
		
		<!-- bi-directional one-to-many association to AimsAuditTrail 
		<set name="aimsAuditTrails" lazy="true" inverse="true">
			<key>
				<column name="USER_ID"/>
			</key>
			<one-to-many class="com.netpace.aims.model.AimsAuditTrail"/>
		</set>
		-->
		<!-- bi-directional one-to-many association to AimsEmailReminderUser 
		<set name="aimsEmailReminderUsers" lazy="true" inverse="true">
			<key>
				<column name="USER_ID"/>
			</key>
			<one-to-many class="com.netpace.aims.model.AimsEmailReminderUser"/>
		</set>
		-->
		<!-- bi-directional one-to-many association to AimsEmailGroupUser 
		<set name="aimsEmailGroupUsers" lazy="true" inverse="true">
			<key>
				<column name="USER_ID"/>
			</key>
			<one-to-many class="com.netpace.aims.model.AimsEmailGroupUser"/>
		</set>
		-->
		<!-- bi-directional one-to-many association to AimsAllianceWhitePaper 
		<set name="aimsAllianceWhitePapers" lazy="true" inverse="true">
			<key>
				<column name="SUBMITTED_BY"/>
			</key>
			<one-to-many class="com.netpace.aims.model.AimsAllianceWhitePaper"/>
		</set>
		-->
		<!-- bi-directional one-to-many association to AimsAllianceContract 
		<set name="aimsAllianceContracts" lazy="true" inverse="true">
			<key>
				<column name="ACCEPT_DECLINE_USER_ID"/>
			</key>
			<one-to-many class="com.netpace.aims.model.AimsAllianceContract"/>
		</set>
		-->
		<!-- bi-directional one-to-one association to AimsUser 
		<one-to-one name="aimsUser" class="com.netpace.aims.model.AimsUser" outer-join="auto" property-ref="aimsUser"/>
		-->
		<!-- bi-directional one-to-one association to AimsUser 
		<many-to-one name="aimsUser" class="com.netpace.aims.model.AimsUser">
			<column name="USER_ID"/>
		</many-to-one>
		-->
		<!-- bi-directional one-to-many association to AimsChatUser 
		<set name="aimsChatUsers" lazy="true" inverse="true">
			<key>
				<column name="USER_ID"/>
			</key>
			<one-to-many class="com.netpace.aims.model.AimsChatUser"/>
		</set>
		-->
		<!-- bi-directional one-to-many association to AimsAllianc 
		<set name="aimsAlliancsByVzwAccountManager" lazy="true" inverse="true">
			<key>
				<column name="VZW_ACCOUNT_MANAGER"/>
			</key>
			<one-to-many class="com.netpace.aims.model.AimsAllianc"/>
		</set>
		-->
		<!-- bi-directional one-to-many association to AimsAllianc 
		<set name="aimsAlliancsByAdminUserId" lazy="true" inverse="true">
			<key>
				<column name="ADMIN_USER_ID"/>
			</key>
			<one-to-many class="com.netpace.aims.model.AimsAllianc"/>
		</set>
		-->
		<!-- uni-directional one-to-many association to AimsVzwDepartment 
		<set name="aimsVzwDepartments" lazy="true">
			<key>
				<column name="DEPT_MANAGER_USER_ID"/>
			</key>
			<one-to-many class="com.netpace.aims.model.AimsVzwDepartment"/>
		</set>
		-->
		<!-- bi-directional one-to-many association to AimsNotificationUser 
		<set name="aimsNotificationUsers" lazy="true" inverse="true">
			<key>
				<column name="USER_ID"/>
			</key>
			<one-to-many class="com.netpace.aims.model.AimsNotificationUser"/>
		</set>
		-->
		<!-- bi-directional one-to-many association to AimsAllianceSalesLead 
		<set name="aimsAllianceSalesLeads" lazy="true" inverse="true">
			<key>
				<column name="SUBMITTED_BY"/>
			</key>
			<one-to-many class="com.netpace.aims.model.AimsAllianceSalesLead"/>
		</set>
		-->
	</class>
</hibernate-mapping>
