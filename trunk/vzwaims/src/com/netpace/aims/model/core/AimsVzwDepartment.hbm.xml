<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping>
	<!-- 
    Created by Middlegen Hibernate plugin

    http://boss.bekk.no/boss/middlegen/
    http://hibernate.sourceforge.net/
-->
	<class name="com.netpace.aims.model.core.AimsVzwDepartment" table="AIMS_VZW_DEPARTMENTS">
		<meta attribute="extends">com.netpace.aims.model.BaseValueObject</meta>
		<id name="departmentId" type="java.lang.Long" column="DEPARTMENT_ID">
			<generator class="sequence">
				<param name="sequence">SEQ_PK_AIMS_VZW_DEPARTMENTS</param>
			</generator>
		</id>
		<property name="departmentName" type="java.lang.String" column="DEPARTMENT_NAME" not-null="true" length="50"/>
		<property name="departmentDesc" type="java.lang.String" column="DEPARTMENT_DESC" length="100"/>
		<!-- associations -->
		<!-- bi-directional one-to-many association to AimsUser -->
		<set name="aimsUsers" lazy="true" inverse="true">
			<key>
				<column name="DEPARTMENT_ID"/>
			</key>
			<one-to-many class="com.netpace.aims.model.core.AimsUser"/>
		</set>
		<!-- bi-directional many-to-one association to AimsAppCategory 
		<many-to-one name="aimsAppCategory" class="com.netpace.aims.model.AimsAppCategory" not-null="true">
			<column name="CATEGORY_ID"/>
		</many-to-one>
		-->
	</class>
</hibernate-mapping>
