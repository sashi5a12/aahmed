<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >
    
<hibernate-mapping>

<!-- 
    Created by Middlegen Hibernate plugin

    http://boss.bekk.no/boss/middlegen/
    http://hibernate.sourceforge.net/
-->

<class 
    name="com.netpace.aims.model.core.AimsContract" 
    table="AIMS_CONTRACTS"
>
    <meta attribute="extends">com.netpace.aims.model.BaseValueObject</meta>
    <id
        name="contractId"
        type="java.lang.Long"
        column="CONTRACT_ID"
    >
        <generator class="sequence">
             <param name="sequence">SEQ_PK_CONTRACTS</param>
        </generator>
    </id>
    
    <property name="contractRefId" type="java.lang.Long" column="CONTRACT_REF_ID" length="22"    />

    <property
        name="contractTitle"
        type="java.lang.String"
        column="CONTRACT_TITLE"
        length="50"
    >
        <meta attribute="use-in-tostring">true</meta>
    </property>
    <property
        name="version"
        type="java.lang.String"
        column="VERSION"
        length="50"
    />
    <property
        name="ifNegotiable"
        type="java.lang.String"
        column="IF_NEGOTIABLE"
        length="1"
    />
    <property
        name="status"
        type="java.lang.String"
        column="STATUS"
        length="20"
    />
	
    <property
        name="document"
        type="java.sql.Blob"
        column="DOCUMENT"
        length="4000"
    />

    <property
        name="documentFileName"
        type="java.lang.String"
        column="DOCUMENT_FILE_NAME"
        length="150"
    />
    <property
        name="documentContentType"
        type="java.lang.String"
        column="DOCUMENT_CONTENT_TYPE"
        length="100"
    />

    <property
        name="ifAttachToSoln"
        type="java.lang.String"
        column="IF_ATTACH_TO_SOLN"
        length="1"
    />
    <property
        name="comments"
        type="java.lang.String"
        column="COMMENTS"
        length="500"
    />
    <property
        name="createdBy"
        type="java.lang.String"
        column="CREATED_BY"
        length="50"
    />
    <property
        name="createdDate"
        type="java.sql.Timestamp"
        column="CREATED_DATE"
        length="7"
    />
    <property
        name="lastUpdatedBy"
        type="java.lang.String"
        column="LAST_UPDATED_BY"
        length="50"
    />
    <property
        name="lastUpdatedDate"
        type="java.sql.Timestamp"
        column="LAST_UPDATED_DATE"
        length="7"
    />


	<property 
		name="platformId" 
		type="java.lang.Long" 
		column="PLATFORM_ID" 
		length="22"
	 />

    <property
        name="expiryDate"
        type="java.sql.Timestamp"
        column="EXPIRY_DATE"
        length="7"
    />       

    <property
        name="isBoboContract"
        type="java.lang.String"
        column="IS_BOBO_CONTRACT"
        length="1"
    />


    <property
    	name="htmlFile"
    	type="java.sql.Blob"
    	column="HTML_FILE"
    	length="4000"
    />


    <property
        name="htmlFileFileName"
        type="java.lang.String"
        column="HTML_FILE_FILE_NAME"
        length="150"
    />
    <property
        name="htmlFileContentType"
        type="java.lang.String"
        column="HTML_FILE_CONTENT_TYPE"
        length="100"
    />

    <property
        name="isClickThroughContract"
        type="java.lang.String"
        column="IS_CLICK_THROUGH_CONTRACT"
        length="1"
    />

    <!-- associations -->
    <!-- bi-directional many-to-one association to AimsPlatform -->


    <many-to-one
        name="aimsPlatform"
        class="com.netpace.aims.model.core.AimsPlatform"
        not-null="true"
		insert="false"
		update="false"
    >
        <column name="PLATFORM_ID" />
    </many-to-one>   
	
    <!-- bi-directional one-to-many association to AimsAllianceContract 
    <set
        name="aimsAllianceContracts"
        lazy="true"
        inverse="true"
    >
        <key>
            <column name="CONTRACT_ID" />
        </key>
        <one-to-many 
            class="com.netpace.aims.model.core.AimsAllianceContract"
        />
    </set>
     -->
</class>
</hibernate-mapping>
