<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >

<hibernate-mapping>
    <class name="com.netpace.aims.model.wapoptout.AimsWapOptoutWhitelistUrl" table="AIMS_WAP_OPTOUT_WHITELIST">
    	<meta attribute="extends">com.netpace.aims.model.BaseValueObject</meta>
        <id name="whitelistId" type="java.lang.Long">
            <column name="WHITELIST_ID"/>
	        <generator class="sequence">
				<param name="sequence">SEQ_PK_WHITELIST_ID</param>
			</generator>
        </id>
        <property name="url" type="java.lang.String">
            <column name="URL" />
        </property>
    </class>
</hibernate-mapping>
