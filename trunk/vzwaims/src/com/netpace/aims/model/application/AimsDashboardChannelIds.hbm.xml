<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >

<hibernate-mapping>
    <class name="com.netpace.aims.model.application.AimsDashboardChannelIds" table="AIMS_DASHBOARD_CHANNEL_IDS">
    	<meta attribute="extends">com.netpace.aims.model.BaseValueObject</meta>
        <id name="channelId" type="java.lang.Long" column="CHANNEL_ID">
            <generator class="assigned" />
        </id>
        <property name="dashboardAppsId" type="java.lang.Long" column="DASHBOARD_APPS_ID" />
        <property name="appTitle" type="java.lang.String" column="APP_TITLE" length="200" />
        <property name="companyName" type="java.lang.String" column="COMPANY_NAME" length="255" />
    </class>
</hibernate-mapping>
