<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >
    
<hibernate-mapping>

<!-- 
    Created by Middlegen Hibernate plugin

    http://boss.bekk.no/boss/middlegen/
    http://hibernate.sourceforge.net/
-->

<class 
    name="com.netpace.aims.model.application.AimsMmsApp" 
    table="AIMS_MMS_APPS"
>
    <meta attribute="extends">com.netpace.aims.model.BaseValueObject</meta>    
    
    <id
        name="mmsAppsId"
        type="java.lang.Long"
        column="MMS_APPS_ID"
    >
        <generator class="assigned"/>
			
    </id>
    
    <property
        name="contentProvider"
        type="java.lang.String"
        column="CONTENT_PROVIDER"
        length="200"
    />
    <property
        name="programPromoInfo"
        type="java.lang.String"
        column="PROGRAM_PROMO_INFO"
        length="500"
    />
    <property
        name="targetedLaunchDate"
        type="java.sql.Timestamp"
        column="TARGETED_LAUNCH_DATE"
        length="7"
    />
    <property
        name="sampleContentFileFileName"
        type="java.lang.String"
        column="SAMPLE_CONTENT_FILE_NAME"
        length="150"
    />
    <property
        name="sampleContentFileContType"
        type="java.lang.String"
        column="SAMPLE_CONTENT_CONTENT_TYPE"
        length="100"
    />
    <property
        name="expectedMsgTraffic"
        type="java.lang.Long"
        column="EXPECTED_MSG_TRAFFIC"
        length="22"
    />

    <!-- associations -->
    <!-- bi-directional one-to-one association to AimsApp 
    <many-to-one
        name="aimsApp"
        class="com.netpace.aims.model.AimsApp"
    >
        <column name="MMS_APPS_ID" />
    </many-to-one>   
		-->
		
</class>
</hibernate-mapping>
