<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >
    
<hibernate-mapping>

<!-- 
    Created by Middlegen Hibernate plugin

    http://boss.bekk.no/boss/middlegen/
    http://hibernate.sourceforge.net/
-->

<class 
    name="com.netpace.aims.model.masters.AimsBrowser" 
    table="AIMS_BROWSERS"
>
<meta attribute="extends">com.netpace.aims.model.BaseValueObject</meta>
    <id
        name="browserId"
        type="java.lang.Long"
        column="BROWSER_ID"
    >
		<generator class="sequence">
			<param name="sequence">SEQ_PK_BROWSERS</param>
		</generator>
    </id>
    <property
        name="browserName"
        type="java.lang.String"
        column="BROWSER_NAME"
        not-null="true"
        length="100"
    />
    <property
        name="browserVersion"
        type="java.lang.String"
        column="BROWSER_VERSION"
        length="20"
    />
    <property
        name="createdBy"
        type="java.lang.String"
        column="CREATED_BY"
        length="50"
    />
    <property
        name="createdDate"
        type="java.sql.Timestamp"
        column="CREATED_DATE"
        length="7"
    />
    <property
        name="lastUpdatedBy"
        type="java.lang.String"
        column="LAST_UPDATED_BY"
        length="50"
    />
    <property
        name="lastUpdatedDate"
        type="java.sql.Timestamp"
        column="LAST_UPDATED_DATE"
        length="7"
    />

    <!-- associations -->
    <!-- bi-directional one-to-many association to AimsWapAppsBrowser -->
	<!--
    <set
        name="aimsWapAppsBrowsers"
        lazy="true"
        inverse="true"
    >
        <key>
            <column name="BROWSER_ID" />
        </key>
        <one-to-many 
            class="com.netpace.aims.model.AimsWapAppsBrowser"
        />
    </set>
	-->
    <!-- bi-directional many-to-one association to AimsPlatform -->
    <many-to-one
        name="aimsPlatform"
        class="com.netpace.aims.model.core.AimsPlatform"
        not-null="true"
    >
        <column name="PLATFORM_ID" />
    </many-to-one>   

</class>
</hibernate-mapping>
