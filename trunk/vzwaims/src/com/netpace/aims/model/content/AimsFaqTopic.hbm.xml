<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >
    
<hibernate-mapping>

<!-- 
    Created by Middlegen Hibernate plugin

    http://boss.bekk.no/boss/middlegen/
    http://hibernate.sourceforge.net/
-->

<class name="com.netpace.aims.model.content.AimsFaqTopic"  table="AIMS_FAQ_TOPICS">
    <meta attribute="extends">com.netpace.aims.model.BaseValueObject</meta>
    <id name="faqTopicId" type="java.lang.Long" column="FAQ_TOPIC_ID">
        <generator class="sequence">
             <param name="sequence">SEQ_PK_FAQ_TOPICS</param>
        </generator>
    </id>
    <property  name="faqTopicQuestion" type="java.lang.String" column="FAQ_TOPIC_QUESTION" length="500">
        <meta attribute="use-in-tostring">true</meta>
    </property>

    <property
        name="faqTopicAnswer"
        type="java.lang.String"
        column="FAQ_TOPIC_ANSWER"
        length="2000"
    >
        <meta attribute="use-in-tostring">true</meta>
    </property>
    <property
        name="faqTopicDesc"
        type="java.lang.String"
        column="FAQ_TOPIC_DESC"
        length="500"
    />
    <property
        name="createdBy"
        type="java.lang.String"
        column="CREATED_BY"
        length="50"
    />
    <property
        name="createdDate"
        type="java.sql.Timestamp"
        column="CREATED_DATE"
        length="7"
    />
    <property
        name="lastUpdatedBy"
        type="java.lang.String"
        column="LAST_UPDATED_BY"
        length="50"
    />
    <property
        name="lastUpdatedDate"
        type="java.sql.Timestamp"
        column="LAST_UPDATED_DATE"
        length="7"
    />

    <!-- associations -->
    <!-- bi-directional many-to-one association to AimsFaqCategory -->
    <many-to-one
        name="aimsFaqCategory"
        class="com.netpace.aims.model.content.AimsFaqCategory"
        not-null="true"
    >
        <column name="FAQ_CATEGORY_ID" />
    </many-to-one>   

</class>
</hibernate-mapping>
