<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >
    
<hibernate-mapping>

<!-- 
    Created by Middlegen Hibernate plugin

    http://boss.bekk.no/boss/middlegen/
    http://hibernate.sourceforge.net/
-->

<class 
    name="com.netpace.aims.model.security.AimsSystemPrivilege" 
    table="AIMS_SYS_PRIVILEGES"
    dynamic-update="true"
>
    <meta attribute="class-description">
       Aims User Priviledges
    </meta>
    <meta attribute="extends">com.netpace.aims.model.BaseValueObject</meta>
    <id
        name="privilegeId"
        type="java.lang.Long"
        column="PRIVILEGE_ID"
    >
        <generator class="sequence">
             <param name="sequence">SEQ_PK_SYS_PRIVILEGES</param>
        </generator>
    </id>
    <property
        name="privilegeName"
        type="java.lang.String"
        column="PRIVILEGE_NAME"
        not-null="true"
        length="100"
    >
        <meta attribute="use-in-tostring">true</meta>
    </property>
    <property
        name="privilegeDescription"
        type="java.lang.String"
        column="PRIVILEGE_DESCRIPTION"
        length="200"
    />
    
    <property
        name="privilegeKey"
        type="java.lang.String"
        column="PRIVILEGE_KEY"
        not-null="true"
        length="100"
    />
    
    <property
        name="availableTo"
        type="java.lang.String"
        column="AVAILABLE_TO"
        length="1"
    />
    <!--
    <property
        name="ifSelectAllowed"
        type="java.lang.String"
        column="IF_SELECT_ALLOWED"
        length="1"
    />
    <property
        name="ifUpdateAllowed"
        type="java.lang.String"
        column="IF_UPDATE_ALLOWED"
        length="1"
    />
    <property
        name="ifInsertAllowed"
        type="java.lang.String"
        column="IF_INSERT_ALLOWED"
        length="1"
    />
    <property
        name="ifDeleteAllowed"
        type="java.lang.String"
        column="IF_DELETE_ALLOWED"
        length="1"
    />
    -->
    <property
        name="createdBy"
        type="java.lang.String"
        column="CREATED_BY"
        length="50"
    />
    <property
        name="createdDate"
        type="java.sql.Timestamp"
        column="CREATED_DATE"
        length="7"
    />
    <property
        name="lastUpdatedBy"
        type="java.lang.String"
        column="LAST_UPDATED_BY"
        length="50"
    />
    <property
        name="lastUpdatedDate"
        type="java.sql.Timestamp"
        column="LAST_UPDATED_DATE"
        length="7"
    />

    <property
        name="menuId"
        type="java.lang.Long"
        column="MENU_ID"
        insert="false"
        update="false"
    />

    <property
        name="subMenuId"
        type="java.lang.Long"
        column="SUB_MENU_ID"
        insert="false"
        update="false"
    />

    <!-- associations -->

    <!-- bi-directional many-to-one association to AimsMenu -->
    <many-to-one name="menu" class="com.netpace.aims.model.security.AimsMenu" not-null="true">
        <column name="MENU_ID" />
    </many-to-one>   

    <many-to-one name="subMenu" class="com.netpace.aims.model.security.AimsSubMenu" not-null="true" >
        <column name="SUB_MENU_ID" />
    </many-to-one>   
    

    <!-- bi-directional one-to-many association to AimsSection -->
    
    <set name="sections"  inverse="true" >
        <key>
            <column name="PRIVILEGE_ID" />
        </key>
        <one-to-many 
            class="com.netpace.aims.model.security.AimsSection"
        />
    </set>


</class>
</hibernate-mapping>
