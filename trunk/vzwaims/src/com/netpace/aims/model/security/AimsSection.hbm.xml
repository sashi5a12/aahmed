<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >
    
<hibernate-mapping>

<!-- 
    Created by Middlegen Hibernate plugin

    http://boss.bekk.no/boss/middlegen/
    http://hibernate.sourceforge.net/
-->

<class 
    name="com.netpace.aims.model.security.AimsSection" 
    table="AIMS_SECTIONS"
>
    <meta attribute="extends">com.netpace.aims.model.BaseValueObject</meta>
    <id
        name="sectionId"
        type="java.lang.Long"
        column="SECTION_ID"
    >
        <generator class="sequence">
             <param name="sequence">SEQ_PK_SECTIONS</param>
        </generator>
    </id>
    <property
        name="sectionName"
        type="java.lang.String"
        column="SECTION_NAME"
        not-null="true"
        length="100"
    >
        <meta attribute="use-in-tostring">true</meta>
    </property>
    <property
        name="sectionDescription"
        type="java.lang.String"
        column="SECTION_DESCRIPTION"
        length="200"
    />

    <property
        name="sectionKey"
        type="java.lang.String"
        column="SECTION_KEY"
        not-null="true"
        length="100"
    />


    <property
        name="createdBy"
        type="java.lang.String"
        column="CREATED_BY"
        length="50"
    />
    <property
        name="createdDate"
        type="java.sql.Timestamp"
        column="CREATED_DATE"
        length="7"
    />
    <property
        name="lastUpdatedBy"
        type="java.lang.String"
        column="LAST_UPDATED_BY"
        length="50"
    />
    <property
        name="lastUpdatedDate"
        type="java.sql.Timestamp"
        column="LAST_UPDATED_DATE"
        length="7"
    />
    <property
        name="fieldName"
        type="java.lang.String"
        column="FIELD_NAME"
        length="100"
    />

    <!-- associations -->
    <!-- bi-directional many-to-one association to AimsSysPrivileg -->
    <!--
    <many-to-one
        name="aimsSysPrivilege"
        class="com.netpace.aims.model.AimsSysPrivileg"
        not-null="true"
    >
        <column name="PRIVILEGE_ID" />
    </many-to-one>   
    -->

</class>
</hibernate-mapping>
