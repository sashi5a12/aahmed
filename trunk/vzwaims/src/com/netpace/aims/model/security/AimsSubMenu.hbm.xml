<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >
    
<hibernate-mapping>

<!-- 
    Created by Middlegen Hibernate plugin

    http://boss.bekk.no/boss/middlegen/
    http://hibernate.sourceforge.net/
-->

<class 
    name="com.netpace.aims.model.security.AimsSubMenu" 
    table="AIMS_SUB_MENUS"
>
    <meta attribute="extends">com.netpace.aims.model.BaseValueObject</meta>
    <id
        name="subMenuId"
        type="java.lang.Long"
        column="SUB_MENU_ID"
    >
        <generator class="sequence">
             <param name="sequence">SEQ_PK_SUB_MENU</param>
        </generator>
    </id>
    <property
        name="subMenuName"
        type="java.lang.String"
        column="SUB_MENU_NAME"
        length="100"
    >
        <meta attribute="use-in-tostring">true</meta>
    </property>
    <property
        name="subMenuUrl"
        type="java.lang.String"
        column="SUB_MENU_URL"
        length="200"
    />
    <property
        name="imageName"
        type="java.lang.String"
        column="IMAGE_NAME"
        length="100"
    />
    <property
        name="sortOrder"
        type="java.lang.Integer"
        column="SORT_ORDER"
        length="22">
        <meta attribute="use-in-tostring">true</meta>
    </property>
    <property
        name="createdBy"
        type="java.lang.String"
        column="CREATED_BY"
        length="50"
    />
    <property
        name="createdDate"
        type="java.sql.Timestamp"
        column="CREATED_DATE"
        length="7"
    />
    <property
        name="lastUpdatedBy"
        type="java.lang.String"
        column="LAST_UPDATED_BY"
        length="50"
    />
    <property
        name="lastUpdatedDate"
        type="java.sql.Timestamp"
        column="LAST_UPDATED_DATE"
        length="7"
    />

    <!-- associations -->
    <!-- bi-directional one-to-many association to AimsSysPrivileg -->
    <!--
    <set name="aimsSysPrivilegs" lazy="true" inverse="true">
        <key>
            <column name="SUB_MENU_ID" />
        </key>
        <one-to-many class="com.netpace.aims.model.AimsSysPrivilege"/>
    </set>
    -->

    <!-- bi-directional many-to-one association to AimsMenu -->
    <many-to-one name="aimsMenu" class="com.netpace.aims.model.security.AimsMenu" not-null="true">
        <column name="MENU_ID" />
    </many-to-one>   
    
</class>
</hibernate-mapping>
