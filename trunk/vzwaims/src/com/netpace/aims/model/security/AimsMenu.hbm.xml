-->

<class name="com.netpace.aims.model.security.AimsMenu" table="AIMS_MENUS">
    <meta attribute="extends">com.netpace.aims.model.BaseValueObject</meta>
    <id
        name="menuId"
        type="java.lang.Long"
        column="MENU_ID"
    >
        <generator class="sequence">
             <param name="sequence">SEQ_PK_MENUS</param>
        </generator>
    </id>
    <property
        name="menuName"
        type="java.lang.String"
        column="MENU_NAME"
        length="100"
    >
        <meta attribute="use-in-tostring">true</meta>
    </property>
    <property
        name="menuUrl"
        type="java.lang.String"
        column="MENU_URL"
        length="200"
    />
    <property
        name="imageName"
        type="java.lang.String"
        column="IMAGE_NAME"
        length="100"
    />
    <property
        name="sortOrder"
        type="java.lang.Integer"
        column="SORT_ORDER"
        length="22"
    />
    <property
        name="createdBy"
        type="java.lang.String"
        column="CREATED_BY"
        length="50"
    />
    <property
        name="createdDate"
        type="java.sql.Timestamp"
        column="CREATED_DATE"
        length="7"
    />
    <property
        name="lastUpdatedBy"
        type="java.lang.String"
        column="LAST_UPDATED_BY"
        length="50"
    />
    <property
        name="lastUpdatedDate"
        type="java.sql.Timestamp"
        column="LAST_UPDATED_DATE"
        length="7"
    />

    <!-- associations -->
    <!-- bi-directional one-to-many association to AimsSubMenu -->
    <set
        name="aimsSubMenus"
        lazy="true"
        inverse="true"
    >
        <key>
            <column name="MENU_ID" />
        </key>
        <one-to-many 
            class="com.netpace.aims.model.security.AimsSubMenu"
        />
    </set>
    <!-- bi-directional one-to-many association to AimsSysPrivileg -->
    <!--
    <set
        name="aimsSysPrivilegs"
        lazy="true"
        inverse="true"
    >
        <key>
            <column name="MENU_ID" />
        </key>
        <one-to-many 
            class="com.netpace.aims.model.AimsSysPrivileg"
        />
    </set>
    -->
</class>
</hibernate-mapping>
