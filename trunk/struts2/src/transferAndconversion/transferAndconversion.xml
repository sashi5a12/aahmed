<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="transferAndconversion" namespace="/conversion" extends="struts-default">
		<action name="ArrayDataTransfer">
			<result>/jsps/transfer_and_conversion/ArrayDataTransfer.jsp</result>
		</action>
		<action name="ArrayDataTransferSubmit" class="transferAndconversion.ArrayDataTransfer">
			<result>/jsps/transfer_and_conversion/ArrayDataTransferSucess.jsp</result>
			<result name="input">/jsps/transfer_and_conversion/ArrayDataTransfer.jsp</result>
		</action>
		
		<action name="ListDataTransfer">
			<result>/jsps/transfer_and_conversion/ListDataTransfer.jsp</result>
		</action>
		<action name="ListDataTransferSubmit" class="transferAndconversion.ListDataTransfer">
			<result>/jsps/transfer_and_conversion/ListDataTransferSucess.jsp</result>
			<result name="input">/jsps/transfer_and_conversion/ListDataTransfer.jsp</result>
		</action>

		<action name="UserDataTransfer">
			<result>/jsps/transfer_and_conversion/UserDataTransfer.jsp</result>
		</action>
		<action name="UserDataTransferSubmit" class="transferAndconversion.UserDataTransfer">
			<result>/jsps/transfer_and_conversion/UserDataTransferSucess.jsp</result>
			<result name="input">/jsps/transfer_and_conversion/UserDataTransfer.jsp</result>
		</action>		
	</package>
</struts>