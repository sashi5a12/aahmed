<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.netpace.vzdn.model">
    <class name="DeviceAnyWhere" table="vzdn_device_anywhere" >
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="native" />
        </id>
        <property name="emailAddress" type="java.lang.String">
            <column name="useremail" length="50" />
        </property>
         <property name="gtmCompanyId" type="java.lang.String">
            <column name="gtm_company_id" length="50"/>
        </property>
	    <property name="datetime" type="java.sql.Timestamp">
            <column name="datetime" length="19" />
        </property>
    </class>
</hibernate-mapping>
