<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE beans PUBLIC '-//SPRING//DTD BEAN//EN' 'http://www.springframework.org/dtd/spring-beans.dtd'>
<beans>
	<!-- Registration of new models -->
	
	<bean id="extension.workflowBootstrap" parent="workflowBootstrap">
		<property name="models">
			<list>
				<value>alfresco/extension/model/VerizonWorkflowModel.xml</value>
			</list>
		</property>
		</bean>
	
	
	
	
	<bean id="parallel.workflowBootstrap" parent="workflowDeployer">
		<property name="labels">
			<list>
			<value>alfresco.extension.customWorkflow</value>
			</list>
		</property>
	</bean>

</beans>